extends index

block content
	h1 #{artist_name}
	br
	button(onclick='goBack()') Go Back
	script.
		function goBack() {
		window.history.back();
		}
	br
	br

	- var x = tracks;
	
	ul
		each track in x
			li= `${track.artist_name} - ${track.track_name}`
			li= track.drive_url
			iframe(frameborder='0' src=track.drive_url)


	

			
