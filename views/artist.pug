extends index2

block content
	h1 #{artist_name}
	br
	button(onclick='goBack()') Go Back
	script.
		function goBack() {
		window.history.back();
		}
	br
	br

	- var x = tracks;
	
	ul
		each track in x
			li= `${track.artist_name} - ${track.track_name}`
			iframe(frameborder='0' width='350' height='150' src=track.drive_url)
			audio(src=track.drive_url, type='audio/mpeg', controls='')
				code audio
