extends index2

block content
	h1 #{artist_name}
	br
	button(onclick='goBack()') Go Back
	script.
		function goBack() {
		window.history.back();
		}
	br
	br

	- var x = tracks;
	
	ul
		each track in x
			.uldiv
				li.tunetext= `${track.artist_name} - ${track.track_name} \n`
				audio(src=track.drive_url, type='audio/mpeg', controls='')
					code audio 
				a(href=track.drive_url) '\nDownload Tune'
			