extends index2

block content
	h1 #{artist_name}
	br
	button(onclick='goBack()') Go Back
	script.
		function goBack() {
		window.history.back();
		}
	br
	br

	- var x = tracks;
	#divy

	script
		x.forEach(myFunction);

		function myFunction(item, index) {
  			document.getElementById("demo").innerHTML += index + ":" + x[index].track_name + "<br>"; 
		}
	
	ul
		each track in x
			.uldiv
				li.tunetext= `${track.artist_name} - ${track.track_name}`
				audio(src=track.drive_url, type='audio/mpeg', controls='')
					code audio 
				a(href=track.drive_url) Download Tune
			